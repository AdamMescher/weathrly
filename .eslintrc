{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "mocha": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-unused-vars": [0],
    "padded-blocks": [0],
    "arrow-body-style": [0],
    "consistent-return": [0],
    "no-alert": [0],
    "max-len": [0]
  },
}
